  },
  "license": "GPL-3.0",
  "bin": {
    "discord-qt": "./index.js"
  },
  "scripts": {
    "build": "webpack -p",
    "start": "qode ./dist/index.js",
    "start:test": "webpack --hide-modules && qode ./dist/index.js",
    "debug": "webpack --hide-modules && qode --inspect ./dist/index.js",
    "start:watch": "nodemon -e js,ts,css,scss --ignore dist/ --ignore node_modules/ --exec npm run debug",
    "postinstall": "node ./downloadNodeGui.js && webpack -p",
    "package": "nodegui-packer --pack ./dist",
    "package:deb": "node ./build-deb.js"
  },
  "dependencies": {
    "@nodegui/nodegui": "^0.24.0",
    "@types/markdown-it": "^10.0.1",
    "@types/node": "^14.0.27",
    "@types/pngjs": "^3.4.2",
    "@types/twemoji": "^12.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "discord.js": "git+https://github.com/ruslang02/discord.js.git",
    "env-paths": "^2.2.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^5.1.0",
    "follow-redirects": "^1.12.1",
    "markdown-it": "^11.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "native-addon-loader": "^2.0.1",
    "open": "git+https://github.com/sindresorhus/open.git",
    "pngjs": "^5.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
