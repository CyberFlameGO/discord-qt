    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "whatwg-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@nodegui/packer": "^1.4.1"
  }
}
